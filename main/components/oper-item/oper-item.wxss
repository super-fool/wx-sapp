.oper-container {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.reduce-frame {
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  border: 1px solid #dddddd;
  box-sizing: border-box;
}
.add-frame {
  height: 60rpx;
  width: 60rpx;
  border-radius: 50%;
  background-color: #b4282d;
  box-sizing: border-box;
}
.oper-number {
  height: 60rpx;
  line-height: 60rpx;
  width: 60rpx;
  font-size: 45rpx;
  padding: 0 10rpx;
  text-align: center;
  box-sizing: border-box;
}
